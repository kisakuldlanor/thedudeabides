<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog">
	<defaultcontrol always="true">450</defaultcontrol>
	<coordinates>
		<left>0</left>
		<top>0</top>
	</coordinates>
	<controls>
		<control type="group">
			<include>dialogeffect</include>
			<depth>DepthDialog+</depth>
			<animation effect="slide" start="0,0" end="0,46" time="0" condition="!Integer.IsGreater(Container(450).NumItems,2)">Conditional</animation>
			<animation effect="slide" start="0,0" end="0,46" time="0" condition="!Integer.IsGreater(Container(450).NumItems,3)">Conditional</animation>
			<animation effect="slide" start="0,0" end="0,46" time="0" condition="!Integer.IsGreater(Container(450).NumItems,4)">Conditional</animation>
			<left>235</left>
			<top>75</top>
			<visible>!Skin.HasSetting(DefaultFavView)</visible>
			<control type="image">
				<description>background image</description>
				<left>0</left>
				<top>0</top>
				<width>850</width>
				<height>550</height>
				<texture border="40">$VAR[SelectBack]</texture>
				<visible>Integer.IsGreater(Container(450).NumItems,4)</visible>
			</control>
			<control type="image">
				<description>background image</description>
				<left>0</left>
				<top>0</top>
				<width>850</width>
				<height>458</height>
				<texture border="40">$VAR[SelectBack]</texture>
				<visible>Integer.IsGreater(Container(450).NumItems,3) + !Integer.IsGreater(Container(450).NumItems,4)</visible>
			</control>
			<control type="image">
				<description>background image</description>
				<left>0</left>
				<top>0</top>
				<width>850</width>
				<height>366</height>
				<texture border="40">$VAR[SelectBack]</texture>
				<visible>Integer.IsGreater(Container(450).NumItems,2) + !Integer.IsGreater(Container(450).NumItems,3)</visible>
			</control>
			<control type="image">
				<description>background image</description>
				<left>0</left>
				<top>0</top>
				<width>850</width>
				<height>274</height>
				<texture border="40">$VAR[SelectBack]</texture>
				<visible>!Integer.IsGreater(Container(450).NumItems,2)</visible>
			</control>
			<control type="image">
				<description>Dialog Header image</description>
				<left>40</left>
				<top>16</top>
				<width>770</width>
				<height>40</height>
				<texture>dialogheader.png</texture>
			</control>
			<control type="label" id="1">
				<description>header label</description>
				<left>40</left>
				<top>20</top>
				<width>770</width>
				<height>30</height>
				<font>font13_title</font>
				<label>$LOCALIZE[1036]</label>
				<align>center</align>
				<aligny>center</aligny>
				<textcolor>selected</textcolor>
				<shadowcolor>black</shadowcolor>
			</control>
			<control type="button">
				<description>Close Window button</description>
				<left>760</left>
				<top>15</top>
				<width>64</width>
				<height>32</height>
				<label>-</label>
				<font>-</font>
				<onclick>PreviousMenu</onclick>
				<texturefocus colordiffuse="$VAR[MainColorOne]">DialogCloseButton-focus.png</texturefocus>
				<texturenofocus>DialogCloseButton.png</texturenofocus>
				<onleft>450</onleft>
				<onright>450</onright>
				<onup>450</onup>
				<ondown>450</ondown>
				<visible>system.getbool(input.enablemouse)</visible>
			</control>
			<include condition="!Skin.HasSetting(DefaultFavView)">FavListDialog</include>
			<control type="scrollbar" id="61">
				<left>570</left>
				<top>65</top>
				<width>25</width>
				<height>464</height>
				<texturesliderbackground border="0,14,0,14">ScrollBarV.png</texturesliderbackground>
				<texturesliderbar border="0,14,0,14">ScrollBarV_bar.png</texturesliderbar>
				<texturesliderbarfocus border="0,14,0,14" colordiffuse="$VAR[MainColorOne]">ScrollBarV_bar_focus.png</texturesliderbarfocus>
				<textureslidernib colordiffuse="$VAR[MainColorOne]">ScrollBarNib.png</textureslidernib>
				<textureslidernibfocus colordiffuse="$VAR[MainColorOne]">ScrollBarNib.png</textureslidernibfocus>
				<onleft>450</onleft>
				<onright>90</onright>
				<ondown>61</ondown>
				<onup>61</onup>
				<showonepage>false</showonepage>
				<orientation>vertical</orientation>
				<visible>Control.IsVisible(450)</visible>
			</control>
			<control type="group">
				<control type="label">
					<description>number of files/pages in list text label</description>
					<left>592</left>
					<top>490</top>
					<width>240</width>
					<height>35</height>
					<font>font12</font>
					<align>center</align>
					<aligny>center</aligny>
					<textcolor>grey</textcolor>
					<label>([COLOR=$VAR[MainColorOne]]$INFO[Container(450).NumItems][/COLOR]) $LOCALIZE[31025] - $LOCALIZE[31024] ([COLOR=$VAR[MainColorOne]]$INFO[Container(450).CurrentPage]/$INFO[Container(450).NumPages][/COLOR])</label>
					<animation effect="slide" start="0,0" end="0,-92" time="0" condition="!Integer.IsGreater(Container(450).NumItems,2)">Conditional</animation>
					<animation effect="slide" start="0,0" end="0,-92" time="0" condition="!Integer.IsGreater(Container(450).NumItems,3)">Conditional</animation>
					<animation effect="slide" start="0,0" end="0,-92" time="0" condition="!Integer.IsGreater(Container(450).NumItems,4)">Conditional</animation>
				</control>
				<control type="grouplist" id="90">
					<left>612</left>
					<top>100</top>
					<onleft>450</onleft>
					<onright>450</onright>
					<control type="button" id="7">
						<description>Cancel button</description>
						<width>200</width>
						<height>40</height>
						<label>222</label>
						<font>font12_title</font>
						<textcolor>white</textcolor>
						<focusedcolor>white</focusedcolor>
						<align>center</align>
						<onclick>PreviousMenu</onclick>
					</control>
				</control>
			</control>
		</control>
		<control type="group">
			<depth>DepthSideBlade</depth>
			<animation effect="slide" start="400,0" end="0,0" time="300" tween="quadratic" easing="out">WindowOpen</animation>
			<animation effect="slide" start="0,0" end="400,0" time="300" tween="quadratic" easing="out">WindowClose</animation>
			<visible>Skin.HasSetting(DefaultFavView)</visible>
			<control type="image">
				<left>400r</left>
				<top>0</top>
				<width>420</width>
				<height>720</height>
				<texture border="15,0,0,0" flipx="true">HomeBladeSub.png</texture>
			</control>
			<control type="button">
				<description>Close Window button</description>
				<left>380r</left>
				<top>0</top>
				<width>64</width>
				<height>32</height>
				<label>-</label>
				<font>-</font>
				<onclick>PreviousMenu</onclick>
				<texturefocus colordiffuse="$VAR[MainColorOne]">DialogCloseButton-focus.png</texturefocus>
				<texturenofocus>DialogCloseButton.png</texturenofocus>
				<onleft>450</onleft>
				<onright>450</onright>
				<onup>450</onup>
				<ondown>450</ondown>
				<visible>system.getbool(input.enablemouse)</visible>
			</control>
			<control type="image">
				<left>370r</left>
				<top>30</top>
				<width>200</width>
				<height>200</height>
				<aspectratio align="center">keep</aspectratio>
				<fadetime>IconCrossfadeTime</fadetime>
				<texture>$INFO[ListItem.Icon]</texture>
				<bordertexture border="8">ThumbShadow.png</bordertexture>
				<bordersize>8</bordersize>
			</control>
			<include condition="Skin.HasSetting(DefaultFavView)">FavListOrig</include>
			<control type="label">
				<description>Page label</description>
				<right>30</right>
				<top>670</top>
				<width>350</width>
				<height>30</height>
				<align>right</align>
				<aligny>center</aligny>
				<font>font12</font>
				<textcolor>grey</textcolor>
				<label>([COLOR=$VAR[MainColorOne]]$INFO[Container(450).NumItems][/COLOR]) $LOCALIZE[31025] - $LOCALIZE[31024] ([COLOR=$VAR[MainColorOne]]$INFO[Container(450).CurrentPage]/$INFO[Container(450).NumPages][/COLOR])</label>
			</control>
		</control>
	</controls>
</window>
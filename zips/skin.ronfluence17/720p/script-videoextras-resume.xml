<?xml version="1.0" encoding="utf-8"?>
<window type="buttonMenu">
	<defaultcontrol>9001</defaultcontrol>
	<include>dialogeffect</include>
	<coordinates>
		<posx>0</posx>
		<posy>0</posy>
	</coordinates>
	<controls>
		<control type="button" id="9001">
			<description>Make sure we hit the first button in the group list and not the close window button</description>
			<posx>-16</posx>
			<posy>-16</posy>
			<width>4</width>
			<height>4</height>
			<label>-</label>
			<font>-</font>
			<onfocus>down</onfocus>
			<texturefocus>-</texturefocus>
			<texturenofocus>-</texturenofocus>
			<onleft>9000</onleft>
			<onright>9000</onright>
			<onup>9000</onup>
			<ondown>2</ondown>
		</control>
		<control type="grouplist" id="9000">
			<posx>470</posx>
			<posy>0</posy>
			<width>340</width>
			<height>720</height>
			<onleft>9000</onleft>
			<onright>9000</onright>
			<onup>9000</onup>
			<ondown>9000</ondown>
			<itemgap>0</itemgap>
			<align>center</align>
			<control type="group" id="1">
				<width>340</width>
				<height>30</height>
				<control type="image">
					<description>background top image</description>
					<posx>0</posx>
					<posy>0</posy>
					<width>340</width>
					<height>30</height>
					<texture border="20,19,20,0">DialogContextTop.png</texture>
				</control>
				<control type="button" id="20">
					<description>Close Window button</description>
					<posx>260</posx>
					<posy>5</posy>
					<width>64</width>
					<height>32</height>
					<label>-</label>
					<font>-</font>
					<onclick>PreviousMenu</onclick>
					<texturefocus colordiffuse="$VAR[MainColorOne]">DialogCloseButton-focus.png</texturefocus>
					<texturenofocus>DialogCloseButton.png</texturenofocus>
					<onleft>2</onleft>
					<onright>13</onright>
					<onup>13</onup>
					<ondown>2</ondown>
					<visible>system.getbool(input.enablemouse)</visible>
				</control>
			</control>
			<control type="group" id="200">
				<width>340</width>
				<height>40</height>
				<control type="image">
					<description>background image</description>
					<left>0</left>
					<top>0</top>
					<width>340</width>
					<height>40</height>
					<texture border="25,5,25,5">DialogContextMiddle.png</texture>
				</control>
				<control type="button" id="2">
					<description>Resume button</description>
					<left>18</left>
					<top>0</top>
					<width>304</width>
					<height>40</height>
					<onleft>400</onleft>
					<onright>1</onright>
					<onup>1</onup>
					<ondown>400</ondown>
					<textcolor>grey2</textcolor>
					<focusedcolor>white</focusedcolor>
					<align>center</align>
					<textwidth>290</textwidth>
					<include>NormalButton2</include>
					<pulseonselect>no</pulseonselect>
					<font>font13</font>
					<label>$ADDON[script.videoextras 32104] </label>
				</control>
			</control>
			<control type="group" id="400">
				<width>340</width>
				<height>40</height>
				<control type="image">
					<description>background image</description>
					<left>0</left>
					<top>0</top>
					<width>340</width>
					<height>40</height>
					<texture border="25,5,25,5">DialogContextMiddle.png</texture>
				</control>
				<control type="button" id="40">
					<description>Start from beginning button</description>
					<left>18</left>
					<top>0</top>
					<width>304</width>
					<height>40</height>
					<onleft>1</onleft>
					<onright>200</onright>
					<onup>200</onup>
					<ondown>1</ondown>
					<textcolor>grey2</textcolor>
					<focusedcolor>white</focusedcolor>
					<align>center</align>
					<textwidth>290</textwidth>
					<include>NormalButton2</include>
					<pulseonselect>no</pulseonselect>
					<font>font13</font>
					<label>$ADDON[script.videoextras 32105]</label>
				</control>
			</control>			
			<control type="image" id="14">
				<description>background bottom image</description>
				<posx>0</posx>
				<width>340</width>
				<height>25</height>
				<texture border="20,0,19,20">DialogContextBottom.png</texture>
			</control>
		</control>
	</controls>
</window>
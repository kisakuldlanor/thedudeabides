<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.program.mafwizard" name="MAF Wizard" version="2017.03.25" provider-name="KISAKUL">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">My Anonymous Friends Wizard</summary>
    <description lang="en">My Anonymous Friends Wizard was created to help the kodi community.  Performs auto maintenance to keep your device running smoothly.  Installs builds from The My Anonymous Friends Team.  Installs Addons selected by the MAF Team.</description>
    <platform>all</platform>
  </extension>
</addon>
<addon id="plugin.program.mafwizard" name="MAF Wizard" version="2017.03.25" provider-name="KISAKUL">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">My Anonymous Friends Wizard</summary>
    <description lang="en">My Anonymous Friends Wizard was created to help the kodi community.  Performs auto maintenance to keep your device running smoothly.  Installs builds from The My Anonymous Friends Team.  Installs Addons selected by the MAF Team.</description>
    <platform>all</platform>
  </extension>
</addon>



<addon
  id="skin.ronfluence"
  version="2017.04.13"
  name="Ronfluence"
  provider-name="KISAKUL">
  <requires>
    <import addon="xbmc.gui" version="5.12.0"/>
	  <import addon="script.favourites" version="6.0.3"/>
	  <import addon="service.skin.widgets" version="0.0.32"/>
	  <import addon="service.library.data.provider" version="0.1.6"/>
	  <import addon="script.randomandlastitems" version="2.2.2"/>
  </requires>
  <extension
    point="xbmc.gui.skin"
    debugging="false"
    effectslowdown="1.0">
    <res width="1280" height="720" aspect="16:9" default="true" folder="720p" />
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Modded by KISAKUL...eXtended MOD of Confluence skin originally by Jezz_X and CCM by Mudislander.</summary>
    <summary lang="en_US">Modded by KISAKUL...eXtended MOD of Confluence skin originally by Jezz_X and CCM by Mudislander.</summary>
    <description lang="en">Ronfluence is a MOD of Xonfluence by Helly Repo (based by Mudislander's CCM).</description>
    <description lang="en_US">Ronfluence is a MOD of Xonfluence by Helly Repo (based by Mudislander's CCM).</description>
    <disclaimer lang="en">Modded for ANONYMOUS KODI. Change it as you wish.</disclaimer>
    <disclaimer lang="en_US">Modded for ANONYMOUS KODI. Change it as you wish.</disclaimer>
    <platform>all</platform>
    <assets>
      <icon>resources/icon.png</icon>
      <fanart>resources/fanart.jpg</fanart>
	    <screenshot>resources/screenshot-01.jpg</screenshot>
	    <screenshot>resources/screenshot-02.jpg</screenshot>
	    <screenshot>resources/screenshot-03.jpg</screenshot>
	    <screenshot>resources/screenshot-04.jpg</screenshot>
	    <screenshot>resources/screenshot-05.jpg</screenshot>
	    <screenshot>resources/screenshot-06.jpg</screenshot>
    </assets>
    <news>Twitter Follow for support:  @_KISAKUL</news>
  </extension>
</addon>




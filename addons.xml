<addon
  id="skin.ronfluence17"
  version="2017.05.27b"
  name="Ronfluence 17"
  provider-name="KISAKUL">
  <requires>
    <import addon="xbmc.gui" version="5.12.0"/>
	  <import addon="script.favourites" version="6.0.3"/>
	  <import addon="service.skin.widgets" version="0.0.32"/>
